{
  "manifest_version": 3,
  "name": "Data collector and crash reporting",
  "description": "Collect screenshot, cookies, storage and network for internal reports.",
  "version": "1.0.0",
  "permissions": [
    "scripting",
    "activeTab",
    "tabs",
    "storage",
    "downloads",
    "cookies",
    "debugger",
    "nativeMessaging",
    "scripting"
  ],
  "host_permissions": [
    "https://*/**",
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  },
  "background": {
    "service_worker": "./service_worker.js",
    "type": "module"
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
